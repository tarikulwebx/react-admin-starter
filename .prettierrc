{
  "printWidth": 120,
  "tabWidth": 4,
  "useTabs": false,
  "trailingComma": "es5",
  "semi": true,
  "singleQuote": false,
  "bracketSpacing": true,
  "arrowParens": "always",
  "jsxSingleQuote": false,
  "bracketSameLine": false,
  "endOfLine": "lf",

  "plugins": ["prettier-plugin-tailwindcss"]
}
